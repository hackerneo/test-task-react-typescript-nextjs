{
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": ".",
    "resolveJsonModule": true,
    "types": [
      "react",
    ],
    "lib": [
      "es6",
      "dom"
    ],
    "allowSyntheticDefaultImports": true,
    "watch": true,
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": false,
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "moduleResolution": "node",
    "isolatedModules": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
